{
  "name": "react-portfolio-site",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "15.0.1"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"yarn start\" \"cd client && yarn start\"",
    "lint": "eslint client/src --ext .ts,.tsx --fix && yarn run prettify",
    "prettify": "yarn prettier \"**/*.*(js|jsx|ts|tsx)\" --ignore-path=.prettierignore --write",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cd client && yarn install && yarn run build"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^5.3.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mongoose": "^5.10.11",
    "nodemon": "^2.0.6",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.4.0",
    "prettier": "^2.0.5"
  }
}
